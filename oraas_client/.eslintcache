age or video\n  handleFileType(fileType = null) {\n    // file type check\n    if(fileType == \"image\") {\n      CurrentFileType = \"image\"\n      return <img src={this.state.output} alt=\"example-output\"></img>\n    } else if(fileType == \"video\") {\n      CurrentFileType = \"video\"\n      return <video controls><source src={this.state.output} type=\"video/mp4\"/></video>\n    } else {\n      // example output image\n      return <img src={this.state.output} alt=\"example-output-default\"></img>\n    }\n  }\n\n  // Show process status(waiting, loading, finished)\n  showProcessingInfo() {\n    if(finished === 2) {\n      finished = 3; // waiting new one\n      return <p><b>Done!!</b></p>\n    }\n\n    if(this.state.processing == true) {\n      // processing...\n      return <div><p>Processing your image...</p><img src={process.env.PUBLIC_URL + '/loading.gif'} alt=\"loading\"></img></div>\n    } else if(finished === 1) {\n      // initial phase\n      return <p>Waiting your input file</p>\n    } else if (finished === 3){\n      // Waiting another input\n      return <p><b>Done!!</b></p>\n    }\n  }\n\n  render() {\n    return (\n      <div className=\"App\">\n        <Router>\n          <Header></Header>\n            <div className=\"App-bg\"></div>\n            <div className=\"App-body\">\n              <Switch>\n\n                {/*  Home Page */}\n                <Route exact path=\"/\">\n                  <div className=\"transbox-left\">\n                    <UploadImage parentCallback={this.onOutputChange}></UploadImage>\n                  </div>\n                  <div className=\"transbox-right\">\n                    <ul className=\"bulletless\">\n                      <li>\n                        <label className=\"form-label\">Default file output example:</label>\n                      </li>\n                      <li>\n                        {this.handleFileType(CurrentFileType)}\n                      </li>\n                      <hr></hr>\n                      <li>\n                        {this.showProcessingInfo()}\n                      </li>\n                    </ul>\n                  </div>\n                </Route>\n\n                {/* Class List */}\n                <Route path=\"/classes\">\n                  <ClassList></ClassList>\n                </Route>\n\n                {/* User Images */}\n                <Route path=\"/myimages\">\n                  <Gallery></Gallery>\n                </Route>\n              </Switch>\n            </div>\n        </Router>\n      </div>\n    );\n  }\n\n}\n\nexport default App;\n","D:\\Users\\atamant\\Desktop\\oraas\\uc_maskeliler\\oraas_client\\src\\components\\header.js",[],"D:\\Users\\atamant\\Desktop\\oraas\\uc_maskeliler\\oraas_client\\src\\index.js",[],["40","41"],"D:\\Users\\atamant\\Desktop\\oraas\\uc_maskeliler\\oraas_client\\src\\components\\uploadImage.js",[],"D:\\Users\\atamant\\Desktop\\oraas\\uc_maskeliler\\oraas_client\\src\\components\\classlist.js",[],"D:\\Users\\atamant\\Desktop\\oraas\\uc_maskeliler\\oraas_client\\src\\components\\userimage.js",["42"],"D:\\Users\\atamant\\Desktop\\oraas\\uc_maskeliler\\oraas_client\\src\\components\\gallery.js",["43"],{"ruleId":"44","severity":1,"message":"45","line":33,"column":44,"nodeType":"46","messageId":"47","endLine":33,"endColumn":46},{"ruleId":"44","severity":1,"message":"45","line":54,"column":17,"nodeType":"46","messageId":"47","endLine":54,"endColumn":19},{"ruleId":"44","severity":1,"message":"45","line":57,"column":24,"nodeType":"46","messageId":"47","endLine":57,"endColumn":26},{"ruleId":"44","severity":1,"message":"45","line":73,"column":30,"nodeType":"46","messageId":"47","endLine":73,"endColumn":32},{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","replacedBy":"51"},{"ruleId":"44","severity":1,"message":"45","line":12,"column":21,"nodeType":"46","messageId":"47","endLine":12,"endColumn":23},{"ruleId":"44","severity":1,"message":"45","line":28,"column":34,"nodeType":"46","messageId"